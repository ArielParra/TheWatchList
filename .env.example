# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcd1234efgh5678

# The Movie Database (TMDB) API Key
EXPO_PUBLIC_TMDB_API_KEY=your_tmdb_api_key_here

# Watch Providers Country Code (ISO 3166-1 alpha-2)
# Common codes: US, ES, MX, GB, FR, DE, IT, BR, AR, etc.
EXPO_PUBLIC_WATCH_PROVIDERS_COUNTRY=US

# Other URL
EXPO_PUBLIC_OTHER_URL=yesmovies.pm